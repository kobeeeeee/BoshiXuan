<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/layout_title_bar"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:id="@+id/left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/register_back"
            android:layout_marginTop="81dp"
            android:layout_marginLeft="19dp" />
        <TextView
            android:id="@+id/set"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置"
            android:layout_alignTop="@+id/left"
            android:layout_centerHorizontal="true"
            android:textColor="@color/boldwhite"
            android:textSize="18sp" />
        <ImageView
            android:id="@+id/list01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/setting_list1"
            android:layout_marginTop="16dp"
            android:layout_below="@+id/include2"/>
        <TextView
            android:id="@+id/share"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="分享"
            android:textSize="18sp"
            android:layout_below="@+id/include2"
            android:layout_marginTop="29dp"
            android:layout_marginLeft="10dp"
            android:textColor="@color/wallet_black"/>
        <TextView
            android:id="@+id/feedback"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="意见反馈"
            android:textSize="18sp"
            android:layout_below="@+id/share"
            android:layout_marginTop="18dp"
            android:layout_alignLeft="@+id/share"
            android:textColor="@color/wallet_black"/>
        <TextView
            android:id="@+id/usehelp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="使用帮助"
            android:textSize="18sp"
            android:layout_below="@+id/feedback"
            android:layout_marginTop="18dp"
            android:layout_alignLeft="@+id/share"
            android:textColor="@color/wallet_black"/>
        <ImageView
            android:id="@+id/list02"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/setting_list2"
            android:layout_below="@+id/list01"
            android:layout_marginTop="15dp" />
        <TextView
            android:id="@+id/contact"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="联系客服"
            android:textSize="18sp"
            android:layout_below="@+id/usehelp"
            android:layout_marginTop="35dp"
            android:layout_alignLeft="@+id/share"
            android:textColor="@color/wallet_black"/>
        <TextView
            android:id="@+id/phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="400-8597-333"
            android:textSize="18sp"
            android:layout_alignTop="@+id/contact"
            android:layout_marginLeft="260dp"/>
        <ImageView
            android:id="@+id/list03"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/setting_list3"
            android:layout_below="@+id/list02"
            android:layout_marginTop="15dp"/>
        <TextView
            android:id="@+id/aboutus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="关于我们"
            android:textSize="18sp"
            android:layout_below="@+id/contact"
            android:layout_marginTop="35dp"
            android:layout_alignLeft="@+id/share"
            android:textColor="@color/wallet_black"/>
        <TextView
            android:id="@+id/clean"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="清理缓存"
            android:textSize="18sp"
            android:layout_below="@+id/aboutus"
            android:layout_marginTop="18dp"
            android:layout_alignLeft="@+id/share"
            android:textColor="@color/wallet_black"/>
        <ImageView
            android:id="@+id/logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/logout_selector"
            android:layout_below="@+id/list03"
            android:layout_marginTop="47.5dp"
            android:layout_centerHorizontal="true"/>
    </RelativeLayout>
</LinearLayout>
