                android:layout_below="@+id/textView56"
                android:layout_alignLeft="@+id/textView56"
                android:layout_alignStart="@+id/textView56" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/recharge_bg3"
            android:layout_below="@+id/relativeLayout10"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="9dp"
            android:layout_marginLeft="12dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="银行卡转账充值（对公账户）"
